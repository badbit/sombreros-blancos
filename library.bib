@article{Birkenstock1996,
abstract = {For the most part, students entering my course in Writing About Literature - and the world of literary analysis - believe that literary works are dead, inflexible, and univocal. Faced with these recurring attitudes about literature, I discovered that the MOO, a text-based virtual reality, is the ideal environment for a literature course. In the MOO the text remains "alive" while insisting upon its diverse readings. Initially the MOO is used for group discussions about the literature being studied. At semester's end the MOO becomes the site of a group project in which the students enter cyberspace as characters they have created. Here they collaboratively write and perform a script as those characters - an act I call "performance scripting."},
